{
  "name": "oms-events",
  "version": "0.21.0",
  "description": "Events-module of the OMS",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test npm run db:setup && jest test/api/*.js --runInBand --forceExit",
    "lint": "node_modules/.bin/eslint .",
    "db:create": "sequelize db:create",
    "db:setup": "sequelize db:drop; sequelize db:create; sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "open-coverage": "open-cli coverage/lcov-report/index.html",
    "cli": "node --experimental-repl-await -r ./cli.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "models/**/*.js",
      "!lib/run.js",
      "!lib/sequelize.js",
      "!lib/logger.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AEGEE/oms-events.git"
  },
  "author": "Nico Westerbeck",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AEGEE/oms-events/issues"
  },
  "homepage": "https://github.com/AEGEE/oms-events#readme",
  "dependencies": {
    "@bugsnag/js": "^6.5.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "express-query-boolean": "^2.0.0",
    "faker": "^4.1.0",
    "file-type": "^12.4.2",
    "fs-extra": "^8.1.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-schedule": "^1.3.2",
    "node-xlsx": "^0.15.0",
    "on-finished": "^2.3.0",
    "pg": "^7.17.1",
    "prom-client": "^11.5.3",
    "read-chunk": "^3.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "codecov": "^3.6.1",
    "commitlint": "^8.3.4",
    "conventional-changelog-cli": "^2.0.25",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "jest-junit": "^10.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^11.7.2",
    "nyc": "^15.0.0",
    "opn-cli": "^5.0.0",
    "timekeeper": "^2.2.0"
  }
}
